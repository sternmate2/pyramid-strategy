{
    "id": null,
    "uid": "stock-price-api",
    "title": "Stock Price API Dashboard",
    "tags": ["stock", "api", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 0,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s", 
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "refresh": "30s",
    "panels": [
      {
        "title": "API Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(stock_api_cache_hits_total[5m]) + rate(stock_api_cache_misses_total[5m])",
            "legendFormat": "{{method}} {{route}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0,
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "title": "API Response Times",
        "type": "graph",
        "targets": [
          {
            "expr": "stock_api_nodejs_eventloop_lag_p99_seconds",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "stock_api_nodejs_eventloop_lag_mean_seconds",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "unit": "s",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "title": "Database Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "stock_api_database_connections_total",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "min": 0,
            "color": {
              "mode": "thresholds",
              "thresholds": {
                "steps": [
                  {
                    "color": "green",
                    "value": null
                  },
                  {
                    "color": "yellow",
                    "value": 15
                  },
                  {
                    "color": "red",
                    "value": 18
                  }
                ]
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 8
        }
      },
      {
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(stock_api_cache_hits_total[5m]) / (rate(stock_api_cache_hits_total[5m]) + rate(stock_api_cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {
              "mode": "thresholds",
              "thresholds": {
                "steps": [
                  {
                    "color": "red",
                    "value": null
                  },
                  {
                    "color": "yellow", 
                    "value": 70
                  },
                  {
                    "color": "green",
                    "value": 90
                  }
                ]
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 8
        }
      },
      {
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "stock_api_nodejs_heap_size_used_bytes",
            "legendFormat": "Heap Used",
            "refId": "A"
          },
          {
            "expr": "stock_api_nodejs_heap_size_total_bytes",
            "legendFormat": "Heap Total",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "unit": "bytes",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "title": "Prices Served by Symbol",
        "type": "table",
        "targets": [
          {
            "expr": "stock_api_cache_hits_total",
            "legendFormat": "{{symbol}}",
            "refId": "A",
            "format": "table"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      }
    ]
}
